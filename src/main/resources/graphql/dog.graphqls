type Dog{
    id: ID!
    name: String!
    breed: String!
    origin: String!
}

type Query{
    findAllDogs:[Dog]!
    findDogById(id: ID!): Dog!
    findBreedById(id: ID!): String!
    findDogBreeds: [String]!
    findDogNames: [String]!
}

type Mutation
    newDog(name: String!, breed: String!, origin: String!): Dog!
    deleteDogBreed(breed:String!): Boolean
    updateDogName(newName: String!, id: ID!): Dog!
}
